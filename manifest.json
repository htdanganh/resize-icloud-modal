{
    "manifest_version": 3,
    "name": "Resize iCloud Modal",
    "version": "1.0.0",
    "description": "Resize iCloud Modal in Data Recovery",
    "permissions": [
        "storage",
        "activeTab",
        "declarativeContent",
        "scripting"
    ],
    "matches": [
        "https://www.icloud.com/recovery/*"
    ],
    "resources": [
        "main.css"
    ]
}